{
  "name": "doomed-utils",
  "version": "0.0.15",
  "description": "",
  "private": false,
  "files": [
    "dist/**",
    ".npmignore",
    ".gitignore",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/functions/index.mjs",
      "require": "./dist/functions/index.js",
      "types": "./dist/functions/index.d.ts"
    },
    "./utils/*": {
      "import": "./dist/functions/*.mjs",
      "require": "./dist/functions/*.js",
      "types": "./dist/functions/*.d.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "utils/*": [
        "dist/utils/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "env NODE_ENV=production tsup --config=tsup.config.ts",
    "lint": "eslint -c .eslintrc.json --ext ts,tsx src/**/*",
    "test": "vitest",
    "release": "pnpm run build && changeset publish"
  },
  "engines": {
    "node": ">16.0.0",
    "pnpm": ">=6"
  },
  "keywords": [],
  "author": "Milan Mladek",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "commitlint": "^19.0.3",
    "eslint": "^8.57.0",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.3.3",
    "vitest": "^0.31.4"
  }
}
